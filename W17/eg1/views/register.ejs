    <div class="container">
        <div class="mt-4">
            <h2>註冊註冊</h2>
            <form action="/home/register" method="post">
                <div class="form-group">
                    Username
                    <input type="text" class="form-control" name="username">
                </div>
                <div class="form-group">
                    Nickname<input type="password" class="form-control" name="nickname">
